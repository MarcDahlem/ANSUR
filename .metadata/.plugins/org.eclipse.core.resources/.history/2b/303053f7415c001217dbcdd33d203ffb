package com.example.streamwithvlc;

public class Camera {

	String name = "", room = "";
	int port;
	
	public Camera(String name, String room, int port){
		this.name = name;
		this.room = room;
		this.port = port;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getRoom() {
		return room;
	}

	public void setRoom(String room) {
		this.room = room;
	}

	public int getPort() {
		return port;
	}

	public void setPort(int port) {
		this.port = port;
	}
	
	public String toString(){
		return new String("Camera: " + name + "\n" + "Room: " + room + "\n" + "Port: " + port);
	}
	
}
